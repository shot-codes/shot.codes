{
	"name": "shotcodes-brutal",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --host",
		"generate": "pnpm exec web-image-gen generate",
		"build": "pnpm exec web-image-gen generate && vite build",
		"build-no-gen": "vite build",
		"build-nogen": "vite build",
		"preview": "vite preview --host",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@octokit/types": "^12.4.0",
		"@stefanprobst/remark-shiki": "^2.2.1",
		"@sveltejs/adapter-auto": "^2.1.1",
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.29.1",
		"@tailwindcss/typography": "^0.5.10",
		"@threlte/core": "^6.1.1",
		"@threlte/extras": "^5.7.0",
		"@types/node": "^20.10.4",
		"@types/three": "^0.155.1",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.55.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.35.1",
		"mdsvex": "^0.11.0",
		"octokit": "^3.1.2",
		"postcss": "^8.4.32",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"prettier-plugin-tailwindcss": "^0.4.1",
		"rehype-raw": "^7.0.0",
		"rehype-slug": "^6.0.0",
		"rehype-stringify": "^10.0.0",
		"remark-parse": "^11.0.0",
		"remark-rehype": "^11.0.0",
		"remark-toc": "^9.0.0",
		"remark-unwrap-images": "^4.0.0",
		"shiki": "^0.14.7",
		"svelte": "^4.2.8",
		"svelte-check": "^3.6.2",
		"tailwindcss": "^3.3.6",
		"three": "^0.159.0",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"unified": "^11.0.4",
		"vite": "^4.5.1",
		"web-image-gen": "^1.1.6",
		"web-image-gen-svelte": "^1.0.6"
	},
	"type": "module"
}
